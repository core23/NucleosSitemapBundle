{
    "name": "core23/sitemap-bundle",
    "type": "symfony-bundle",
    "description": "Core23SitemapBundle",
    "homepage": "https://core23.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Gripp",
            "email": "mail@core23.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/common": "^2.8",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/expression-language": "^3.4 || ^4.0",
        "symfony/framework-bundle": "^3.4 || ^4.0",
        "symfony/options-resolver": "^3.0 || ^4.0"
    },
    "conflict": {
        "doctrine/cache": "<1.7",
        "doctrine/doctrine-cache-bundle": "<1.3"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.2",
        "doctrine/cache": "^1.7",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "sllh/composer-lint": "^1.0",
        "symfony/phpunit-bridge": "^4.0"
    },
    "suggest": {
        "doctrine/doctrine-cache-bundle": "If you need caching."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Core23\\SitemapBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core23\\SitemapBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@composer bin all install --ansi"
        ],
        "post-update-cmd": [
            "@composer bin all update --ansi"
        ]
    }
}
