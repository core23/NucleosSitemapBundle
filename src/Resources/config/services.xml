<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    <service id="core23_sitemap.service.manager" class="Core23\SitemapBundle\Sitemap\SitemapServiceManager"/>
    <service id="core23_sitemap.loader.service" class="Core23\SitemapBundle\Loader\ServiceLoader">
      <argument type="collection"/>
    </service>
    <service id="core23_sitemap.manager" class="Core23\SitemapBundle\Model\SitemapManager">
      <argument type="service" id="core23_sitemap.loader.service"/>
    </service>
    <service id="core23_sitemap.generator" class="Core23\SitemapBundle\Generator\SitemapGenerator">
      <argument type="service" id="core23_sitemap.service.manager"/>
      <argument type="service" id="core23_sitemap.manager"/>
      <argument type="service" id="doctrine_cache.providers.core23_sitemap" on-invalid="ignore"/>
    </service>
  </services>
</container>
